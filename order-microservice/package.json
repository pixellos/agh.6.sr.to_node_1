{
  "name": "order-microservice",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.1.4",
    "@types/uuid": "^8.3.0",
    "applicationinsights": "^1.8.7",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "livereload": "^0.9.1",
    "mongoose": "^5.10.9",
    "nstall": "^0.2.0",
    "swagger-ui-express": "^4.1.4",
    "tsoa": "^3.3.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.11.8",
    "@types/swagger-ui-express": "^4.1.2",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.5",
    "openapi-typescript-codegen": "^0.5.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/**",
      "src/generated/**"
    ]
  },
  "scripts": {
    "dev": "nodemon --verbose",
    "generate": "openapi --input ./src/generated/swagger.json --output ./../frontend/src/order-client",
    "generate2": "openapi-generator-cli generate -g typescript-angular -i ./src/generated/swagger.json -o ./../frontend/src/client-order",
    "build": "tsc",
    "start": "nodemon index.ts",
    "serve": "node dist/src/server.js"
  }
}
